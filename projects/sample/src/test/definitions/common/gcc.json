{
  // common query file for gcc-style compilers
  "query": {
    "-E -dM -v ${tmp:c}": {
      "defines": "^#define (\\S*) ?(.*)?$", // match defines in output 
      "includePath": "#include <...> search starts here:\n((?: [^\n]*\n)*)*", // match the lines for include folders 
      "gcc-options": "^COLLECT_GCC_OPTIONS=(.*)", // get the implicit gcc options 
      "target": "^Target: (.*)", // get the target 
      "compiler-path": "^COMPILER_PATH=(.*)", // get the compiler path  
      "library-path": "^LIBRARY_PATH=(.*)",
      "cStandard": "^#define __STDC_VERSION__\\s*(.*)",
      "cppStandard": "^#define __cplusplus\\s*(.*)",
    },
    "--version": {
      "version": "\\S*\\s+\\(.*\\)\\s+([\\d\\.]+)", // parse the version from the output 
    },
  },
  // how do we specify args to trim off if the discovery is handled/initiated by someone else
}