{
  "name": "Clang C++",
  "import": [
    "common/defaults.json",
    "common/clang.json",
  ],
  "discover": {
    "binary": "clang\\+\\+",
    "requirements": {
      "version": {
        "regex": "^clang (\\d+[.]\\d+[.]\\d+)(?:-\\S*)?$"
      },
    }
  },
  "defaults": {
    "target": {
      "compiler": "clang",
      "architecture": "x64",
      "bits": 64,
    }
  }
}