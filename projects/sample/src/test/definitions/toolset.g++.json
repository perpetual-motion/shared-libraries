{
  "name": "GNU C++ [x64]",
  "import": [
    "common/gcc.json",
    "common/defaults.json"
  ],
  "discover": {
    "binary": "g\\+\\+",
    "requirements": {
      "version": {
        "regex": "^(\\d+[.]\\d+[.]\\d+)(?:-\\S*)?$"
      },
    }
  },
  "defaults": {
    "target": {
      "compiler": "gcc",
      "architecture": "x64",
      "bits": 64,
    }
  }
}