{
  "name": "GNU C++ [ARM]",
  "import": "common/gcc.json",
  "discover": {
    "binary": "arm-none-eabi-g\\+\\+",
    "locations": [
      "c:/nxp/",
      "c:/st/"
    ],
    "requirements": {
      "version": {
        "regex": "^(\\d+[.]\\d+[.]\\d+)(?:-\\S*)?$"
      },
      "root": {
        "folder": "${binary}/../../"
      },
    }
  },
  "defaults": {
    "target": {
      "compiler": "gcc",
      "architecture": "arm",
      "bits": 32,
      "platform": "none"
    }
  }
}